{
  "name": "deployhub-backend",
  "version": "1.0.0",
  "description": "DeployHub secure backend for AWS role assumption",
  "main": "assume-role.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/local-server.js",
    "dev": "tsc && node dist/local-server.js",
    "package": "zip -r deployhub-lambda.zip assume-role.js node_modules/",
    "deploy": "aws lambda update-function-code --function-name deployhub-assume-role --zip-file fileb://deployhub-lambda.zip"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.873.0",
    "@aws-sdk/client-s3": "^3.872.0",
    "@aws-sdk/client-sts": "^3.873.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "unzipper": "^0.12.3",
    "@supabase/supabase-js": "^2.55.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.119",
    "@types/node": "^20.5.0",
    "@types/unzipper": "^0.10.11",
    "typescript": "^5.1.6"
  }
}
