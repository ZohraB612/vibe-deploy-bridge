# Environment variables for DeployHub

# Development/Production Mode
NODE_ENV=development

# Application Configuration
VITE_APP_NAME=DeployHub
VITE_APP_VERSION=1.0.0
VITE_API_BASE_URL=http://localhost:3001/api

# OAuth Configuration (Future Implementation)
VITE_GOOGLE_CLIENT_ID=your_google_client_id
VITE_MICROSOFT_CLIENT_ID=your_microsoft_client_id
VITE_GITHUB_CLIENT_ID=your_github_client_id

# Supabase Configuration (Real Authentication)
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Firebase Configuration (Alternative)
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_firebase_auth_domain
VITE_FIREBASE_PROJECT_ID=your_firebase_project_id

# DeployHub Backend API Configuration
VITE_DEPLOYHUB_API_URL=https://your-api-gateway-url.execute-api.us-east-1.amazonaws.com/prod

# Analytics (Future Implementation)
VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
VITE_HOTJAR_ID=your_hotjar_id

# Monitoring (Future Implementation)
VITE_SENTRY_DSN=your_sentry_dsn

# Backend Configuration (when implemented)
DATABASE_URL=postgresql://user:password@localhost:5432/deployhub
REDIS_URL=redis://localhost:6379
JWT_SECRET=your_super_secret_jwt_key
DB_USER=deployhub
DB_PASSWORD=your_secure_password
